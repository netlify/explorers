    "next-mdx-remote": "^1.0.0",
    "next-on-netlify": "^2.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "simple-oauth2": "^4.1.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "husky": "^4.3.0",
    "netlify-cli": "^2.65.1",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\""
